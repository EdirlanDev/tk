AddType text/css .css
AddType text/javascript .js
AddType text/javascript .mjs

<FilesMatch "\.(css|js|mjs|png|jpg|jpeg|gif|webp|svg|mp4|woff2?)$">
  Require all granted
</FilesMatch>

Options -MultiViews
RewriteEngine On
RewriteBase /
RewriteCond %{REQUEST_URI} !\.(css|js|mjs|png|jpg|jpeg|gif|webp|svg|mp4|woff2?)$
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.html [L]
